%nav#top_nav.navbar.navbar-fixed-top
  .container
    .navbar-header
      %button.navbar-toggle{"data-target" => ".navbar-collapse", "data-toggle" => "collapse", type: "button"}
        %span.sr-only Toggle navigation
        %span.icon-bar
        %span.icon-bar
        %span.icon-bar
      = link_to image_tag("logo.svg", class:"img-responsive"), root_path, class: 'navbar-brand', id: 'nav_logo'
    .collapse.navbar-collapse
      %ul.nav.navbar-nav.navbar-left= render 'layouts/supplemental_links'
      %ul.nav.navbar-nav.navbar-right= render 'layouts/navigation_links'